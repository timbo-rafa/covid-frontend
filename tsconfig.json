{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@geo-utils": [
        "./src/utils/geo"
      ],
      "@type-utils": [
        "./src/utils/type"
      ],
      "@covid-api": [
        "./src/covid-api"
      ]
    },
    "jsxImportSource": "react",
  },
  "include": [
    "src"
  ]
}